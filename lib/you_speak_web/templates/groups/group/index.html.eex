<h5>Listing groups</h5>

<ul class="collection">
  <%= for group <- @groups do %>
    <li class="collection-item">
      <%= link group.name, to: Routes.group_path(@conn, :show, group) %>
    </li>
  <% end %>
</ul>

<div class="fixed-action-btn">
  <%= link "Add", to: Routes.group_path(@conn, :new), class: "btn-floating btn-large waves-effect waves-light red" %>
</div>
